<h1>Create a New Event</h1>
<%= form_for @event do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :event_type %>
  <%= f.text_field :event_type, collection: Event::EVENTTYPES %>
  <%= f.label :start %>
  <%= f.date_field :start %>
  <%= f.label :end %>
  <%= f.date_field :end %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.submit %>
<% end %>
